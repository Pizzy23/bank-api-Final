ALTER TABLE `banco`.`usuario` 
CHARACTER SET = utf8 , COLLATE = utf8_general_ci ,
CHANGE COLUMN `id` `id` BIGINT(20) NOT NULL ,
ADD UNIQUE INDEX `nome_UNIQUE` (`nome` ASC) VISIBLE;
;

CREATE TABLE IF NOT EXISTS `banco`.`conta` (
  `id` INT(11) NOT NULL AUTO_INCREMENT,
  `saldo` BIGINT(20) NOT NULL DEFAULT '0',
  `conta_usuario` BIGINT(20) NOT NULL,
  PRIMARY KEY (`id`),
  INDEX `fk_Banco_usuario_idx` (`conta_usuario` ASC) VISIBLE,
  CONSTRAINT `fk_Banco_usuario`
    FOREIGN KEY (`conta_usuario`)
    REFERENCES `banco`.`usuario` (`id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8;

DROP TABLE IF EXISTS `banco`.`conta` ;


SET SQL_MODE=@OLD_SQL_MODE;
SET FOREIGN_KEY_CHECKS=@OLD_FOREIGN_KEY_CHECKS;
